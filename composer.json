{
    "name": "ibrows/hmac-bundle",
    "type": "symfony-bundle",
    "description": "Symfony HMAC Bundle adds a SecurityListenerFactor which provide a hmac authentication for configured firewall",
    "keywords": ["HMAC","Security","SecurityListenerFactor","Symonfy2"],
    "homepage": "http://www.ibrows.ch",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Steiner",
            "homepage": "https://github.com/ibrows"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/security-bundle": "~2.7|~3.0",
        "symfony/framework-bundle": "~2.7|~3.0",
        "acquia/http-hmac-php": "~1.0"
    },
    "require-dev": {
      "phpunit/phpunit": "~4.0",
      "symfony/browser-kit": "~2.7"
    },
    "autoload": {
        "psr-4": { "Ibrows\\HmacBundle\\": "" }
    }
}
